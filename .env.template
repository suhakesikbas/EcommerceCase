# Production Environment Variables Template
# Copy this file to .env and update with your production values

# Database Configuration
POSTGRES_DB=EcommerceApp
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_postgres_password_here
DATABASE_CONNECTION_STRING=Host=postgres;Database=EcommerceApp;Username=postgres;Password=your_secure_postgres_password_here

# RabbitMQ Configuration
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=your_secure_rabbitmq_password_here

# Application Configuration
ASPNETCORE_ENVIRONMENT=Production

# Logging Configuration
LOGGING_LEVEL=Information